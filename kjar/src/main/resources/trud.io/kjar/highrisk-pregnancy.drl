package trud.io.kjar;

import com.sbnz.trud.io.model.Pragnancy;


rule "High risk pragnancy - number of miscarriages"
    when
        $pragnancy: Pragnancy( highRiskPregnancy == false, patient.numberOfMiscarriage > 0 )
    then
        modify($pragnancy) {
        	setHighRiskPregnancy(true);
        }
end